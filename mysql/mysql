・mysql起動
mysqlにはrootでログオンする

user: sebas/sebas

create user sebas@localhost IDENTIFIED BY sebas;

・データベース作成
database: members

table: email
create table email ( nname varchar(15) not null, mail varchar(20) not null primary key, time timestamp not null );

INSERT INTO email(nname, mail, time) VALUES ("staro", "sataro", now());




